                android:textSize="18sp"
                android:textColor="#000000"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.1"
                android:textSize="18sp"
                android:textColor="#000"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text=":"/>
